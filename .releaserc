{
  "preset": "eslint",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/github"
  ],
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    "next",
    { "name": "beta", "prerelease": true },
    { "name": "alpha", "prerelease": true }
  ],
  "verifyConditions": [
    "@semantic-release/npm",
    "@semantic-release/github"
  ],
  "prepare": [
    "@semantic-release/npm"
  ],
  "publish": [
    {
      "path": "@semantic-release/npm",
      "npmPublish": true,
      "tarballDir": "dist"
    },
    "@semantic-release/github"
  ],
  "success": false,
  "fail": false,
  "tagFormat": "v${version}",
  "npmPublish": true,
  "skip": {
    "tag": false
  }
}
